{
  "name": "css-helicopter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "sass:normal": "sass sass/style.scss src/style.css -w",
    "sass:minified": "sass sass/style.scss src/style.min.css -w --style compressed",
    "sass:dev": "concurrently \"yarn sass:normal\" \"yarn sass:minified\""
  },
  "devDependencies": {
    "concurrently": "^7.2.2"
  }
}
